<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.fastrecruiting" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="FirebasePlugin">
        <param name="android-package" value="org.apache.cordova.firebase.FirebasePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="android-package" value="io.ionic.keyboard.IonicKeyboard" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <name>FastRecruiting</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="resources/android/icon/logoApp.png" />
    <icon density="mdpi" src="resources/android/icon/logoApp.png" />
    <icon density="hdpi" src="resources/android/icon/logoApp.png" />
    <icon density="xhdpi" src="resources/android/icon/logoApp.png" />
    <icon density="xxhdpi" src="resources/android/icon/logoApp.png" />
    <icon density="xxxhdpi" src="resources/android/icon/logoApp.png" />
    <splash density="land-ldpi" src="resources/android/splash/splash.png" />
    <splash density="land-mdpi" src="resources/android/splash/splash.png" />
    <splash density="land-hdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xhdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xxhdpi" src="resources/android/splash/splash.png" />
    <splash density="land-xxxhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-ldpi" src="resources/android/splash/splash.png" />
    <splash density="port-mdpi" src="resources/android/splash/splash.png" />
    <splash density="port-hdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xxhdpi" src="resources/android/splash/splash.png" />
    <splash density="port-xxxhdpi" src="resources/android/splash/splash.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="40000" />
    <preference name="loadUrlTimeoutValue" value="40000" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
</widget>
